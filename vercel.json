{
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "frontend/pages/api/**": {
      "maxDuration": 60
    }
  },
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url"
  },
  "envPrefix": "NEXT_PUBLIC_"
}
